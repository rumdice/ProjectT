{
    // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
    // 기존 특성에 대한 설명을 보려면 가리킵니다.
    // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "start game_svr",
            "preLaunchTask": "Compile tsc game",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\out\\game_svr\\index.js",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "console",
            "env": {
                "NODE_ENV": "local",    // local 환경 디버깅:local, 개발서버: dev, 라이브: live
                // "PORT": "5005",
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "start chat_svr",
            "preLaunchTask": "Compile tsc chat",
            "program": "${workspaceFolder}\\out\\chat_svr\\index.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "NODE_ENV": "local",    // local 환경 디버깅:local, 개발서버: dev, 라이브: live
                // "PORT": "5005",
            }
        },
    ]
}